<?xml version="1.0" ?>

<robot name="soccerbot" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:include filename="$(find soccer_description)/models/soccerbot/model.xacro" />

    <link name="world" />
    <joint name="world_base_link" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin rpy="0  0 0" xyz="0 0 0"/>
    <axis xyz="1  0  0"/>
    </joint>

</robot>
